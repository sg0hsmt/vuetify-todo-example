{
  "name": "vuetify-todo-example",
  "version": "0.2.2",
  "private": true,
  "scripts": {
    "serve": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "typecheck": "vue-tsc --noEmit",
    "test:unit": "vitest run",
    "lint": "eslint \"./src/**/*.{ts,tsx,vue}\" \"./tests/**/*.ts\" && prettier --check \"./src/**/*.{ts,tsx,vue}\" \"./tests/**/*.ts\"",
    "lint:fix": "eslint \"./src/**/*.{ts,tsx,vue}\" \"./tests/**/*.ts\" --fix && prettier --write \"./src/**/*.{ts,tsx,vue}\" \"./tests/**/*.ts\""
  },
  "dependencies": {
    "@vue/composition-api": "^1.4.5",
    "vue": "^2.6.14",
    "vue-router": "^3.5.3",
    "vuetify": "^2.6.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/test-utils": "^1.3.0",
    "eslint": "^8.8.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.4.0",
    "jsdom": "^19.0.0",
    "prettier": "^2.5.1",
    "sass": "~1.32.0",
    "typescript": "^4.5.5",
    "unplugin-vue-components": "^0.17.15",
    "vite": "^2.7.13",
    "vite-plugin-vue2": "^1.9.3",
    "vitest": "^0.2.5",
    "vue-template-compiler": "^2.6.14",
    "vue-tsc": "^0.31.1"
  }
}
